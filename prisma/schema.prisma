generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SyncMapping {
  id                Int    @id @unique @default(autoincrement())
  type              String
  sourceServer      String
  sourceCardID      String
  destinationServer String
  destinationCardID String

  @@unique([sourceCardID, destinationCardID])
  @@unique([sourceCardID, destinationServer])
}
